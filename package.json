{
  "name": "@konstant/payload",
  "version": "1.0.67",
  "description": "Payload CMS helpers and utils",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./fields": "./dist/fields.js",
    "./custom": "./dist/custom.js",
    "./utils": "./dist/utils.js",
    "./components": "./dist/components.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm clean && tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "prepare": "husky",
    "ci:versionBump": "pnpm version patch",
    "ci:getVersion": "node -p \"require('./package.json').version\" --silent",
    "reinstall": "rm -rf node_modules && rm -rf pnpm-lock.yaml && pnpm install"
  },
  "dependencies": {
    "@payloadcms/next": "latest",
    "@payloadcms/richtext-lexical": "latest",
    "@payloadcms/ui": "latest",
    "next": "15.1.2",
    "payload": "latest",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "sharp": "0.32.6",
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^22.5.4",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.1",
    "esbuild-sass-plugin": "^3.3.1",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-react": "^7.37.2",
    "globals": "^15.13.0",
    "husky": "^9.1.7",
    "prettier": "^3.2.5",
    "sass": "^1.81.0",
    "tsup": "^8.3.5",
    "typescript": "latest",
    "typescript-eslint": "^8.17.0"
  },
  "packageManager": "pnpm@9.0.0"
}
